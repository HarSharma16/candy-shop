{
  "version": 2,
  "builds": [
    {
      "src": "backend/src/handler.ts",
      "use": "@vercel/node"
    },
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/src/handler.ts"
    }
  ]
}
